<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<BorderPane fx:id="formacionView" prefHeight="500.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1">
   <center>
      <HBox maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" prefHeight="100.0" prefWidth="200.0" BorderPane.alignment="CENTER">
         <children>
            <TableView fx:id="formacionTable" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" prefHeight="500.0" prefWidth="595.0" HBox.hgrow="ALWAYS">
              <columns>
                <TableColumn fx:id="desdeColumna" prefWidth="75.0" text="Desde" />
                <TableColumn fx:id="hastaColumna" prefWidth="114.4000244140625" text="Hasta" />
                  <TableColumn fx:id="denominacionColumna" prefWidth="119.19993591308594" text="Denominación" />
                  <TableColumn fx:id="organizadorColumna" prefWidth="116.800048828125" text="Organizador" />
              </columns>
               <HBox.margin>
                  <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
               </HBox.margin>
            </TableView>
            <VBox maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" prefHeight="500.0" prefWidth="162.0">
               <children>
                  <Button minHeight="25.0" minWidth="60.0" mnemonicParsing="false" onAction="#onAnniadirFormacionAction" text="Añadir">
                     <VBox.margin>
                        <Insets left="5.0" right="5.0" top="5.0" />
                     </VBox.margin>
                  </Button>
                  <Button minHeight="25.0" minWidth="60.0" mnemonicParsing="false" onAction="#onEliminarFormacionAction" text="Eliminar">
                     <VBox.margin>
                        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                     </VBox.margin>
                  </Button>
               </children>
               <HBox.margin>
                  <Insets />
               </HBox.margin>
            </VBox>
         </children>
      </HBox>
   </center>
</BorderPane>
